{
  "env": {
    "es2020": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["jest"],
  "rules": {
    "import/no-dynamic-require": "off",
    "import/prefer-default-export": "off",
    "linebreak-style": ["error", "unix"],
    "no-unused-vars": "error"
  }
}
