{
  "name": "bff-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "babel ./index.js --out-dir dist",
    "dev": "nodemon --exec babel-node index.js",
    "env:terminate": "eb terminate",
    "env:create": "eb create proxy -v -r eu-west-1 -c aleksei-bulgak-study-bff-api --single",
    "env:init": "eb init aleksei-bulgak-study-bff-api --platform node.js --region eu-west-1 -v",
    "env:deploy": "eb deploy"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "aws-sdk": "^2.808.0",
    "axios": "^0.21.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "node-cache": "^5.1.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
